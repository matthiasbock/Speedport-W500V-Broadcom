var rebootconfirm="Um die Änderungen zu übernehmen, wird ein Neustart durchgeführt.Möchten Sie das wirklich?";
var enblServiceID='<%ejGet(serviceId)%>';
var wan_test_status='<%ejGet(wan_test_status)%>';
var redial_time_tonline='<%ejGet(redial_time_tonline)%>';
var redial_time_dyn='<%ejGet(redial_time_dyn)%>';
var redial_time_fes='<%ejGet(redial_time_fes)%>';
var redial_time_another='<%ejGet(redial_time_another)%>';
function alert_wan_test_status()
{
	if (wan_test_status == '2')
		alert("Die Prüfung der Internetverbindung ist fehlgeschlagen. Die Anmeldung\n" +
						"beim Internetanbieter ist gescheitert.\n\n" +
						"Klicken Sie auf \"OK\", um die Zugangsdaten zu überprüfen und zu\n" +
						"korrigieren.\n\n");
	else if (wan_test_status == '3')
		alert("Die Prüfung der Internetverbindung ist fehlgeschlagen. Die\n" +
      						"Verbindung ins Internet ist gesperrt.\n\n" +
      						"Klicken Sie auf 'Abbrechen' und anschließend auf 'Beenden',\n" +
      						"um auf der Startseite den Internetzugang frei zu schalten.\n" + 
                  "Rufen Sie dann den Assistenten erneut auf.\n\n" +
      						"Wenn Sie ohne funktionsfähigen Internetzugang fortfahren\n" +
      						"möchten, dann klicken Sie auf 'OK'.\n\n" );
	else if (wan_test_status == '4')
		alert("Die Prüfung der Internetverbindung ist fehlgeschlagen. Bitte überprüfen,\n" +
			    "ob die T-DSL LED an Ihrem Speedport grün leuchtet, falls nicht\n" +
			    "überprüfen Sie bitte die Steckverbindungen an Ihrem Speedport.\n\n" +
			    "Klicken Sie auf \"OK\", um die Zugangsdaten zu überprüfen und zu\n" +
			    "korrigieren.\n\n");
	
	return;
}
function chk(){
   var onDemand = new Array(0,0,0,0);
   var isIdleTimeOut = new Array(0,0,0,0);

   top.frames[0].ActiveServiceID = enblServiceID;
   if (enblServiceID == 0 ) {
	document.F1.D1.selectedIndex = enblServiceID; 
   }else
        document.F1.D1.selectedIndex = enblServiceID-1;

   //T-online.
   document.ZDAT.P1.value = top.frames[0].cfgprovtxt1[0];
   document.ZDAT.T1.value = '<%ejGet(T_onLinePppNumber)%>';
   //leander debug
   document.ZDAT.P2.value = top.frames[0].cfgprovpwd[0];
   document.ZDAT.P3.value = top.frames[0].cfgprovpwd[0];

   document.ZDAT.T2.value = '<%ejGet(T_onLinePppSufix)%>';
   document.ZDAT.T3.value = '<%ejGet(T_onLineEthMtu)%>';
   
   isIdleTimeOut[0] = '<%ejGet(T_onLineIsIdleTimeOut)%>';//1, manual dialing and disconnecting;0,allow autoconnection and timeout mechnism 
   onDemand[0] = '<%ejGet(T_onLineOnDemand)%>';//auto connect when there comes a request
   if (isIdleTimeOut[0] == '1')
      document.ZDAT.R1[1].checked = true;
   else
      document.ZDAT.R1[0].checked = true;

   if (onDemand[0] == '1') {
       document.ZDAT00.C1.checked = true;
   } else {
       document.ZDAT00.C1.checked = false;
   }
   document.ZDAT00.D1.selectedIndex = '<%ejGet(T_onLinePppIdleTimeOut)%>';
   document.ZDAT01.D2.selectedIndex = redial_time_tonline;
   
   top.frames[0].tonline_password1 = document.ZDAT.P1.value; 
   top.frames[0].tonline_number = document.ZDAT.T1.value;
   top.frames[0].tonline_suffix = document.ZDAT.T2.value;
   top.frames[0].tonline_password = document.ZDAT.P2.value;
   top.frames[0].tonline_mtu = document.ZDAT.T3.value;
   top.frames[0].tonline_persist = isIdleTimeOut[0];
   top.frames[0].tonline_ondemand = onDemand[0];
   top.frames[0].tonline_timeout = document.ZDAT00.D1.selectedIndex;

   //T-business dynamic ip.
   document.ZDAT1.T11.value = '<%ejGet(T_BusinessDynUserName)%>';
   document.ZDAT1.P11.value = top.frames[0].cfgprovpwd[1];
   document.ZDAT1.P12.value = top.frames[0].cfgprovpwd[1];
   
   document.ZDAT1.T12.value = '<%ejGet(T_BusinessDynMtu)%>';
   
   isIdleTimeOut[1] = '<%ejGet(T_BusinessDynIsIdleTimeOut)%>';
   onDemand[1] = '<%ejGet(T_BusinessDynOnDemand)%>';  
   if (isIdleTimeOut[1] == '1' )
      document.ZDAT1.R11[1].checked = true;
   else
   	document.ZDAT1.R11[0].checked = true;
   
   if (onDemand[1] == '1') {
      document.ZDAT1.C11.checked = true;
   } else {
      document.ZDAT1.C11.checked = false;
   }
   document.ZDAT1.D11.selectedIndex = '<%ejGet(T_BusinessDynTimeOut)%>';
   top.frames[0].tbusdyn_name = document.ZDAT1.T11.value;
   top.frames[0].tbusdyn_password = document.ZDAT1.P11.value;
   top.frames[0].tbusdyn_mtu = document.ZDAT1.T12.value;
   top.frames[0].tbusdyn_persist = isIdleTimeOut[1];
   top.frames[0].tbusdyn_ondemand = onDemand[1];
   top.frames[0].tbusdyn_timeout = document.ZDAT1.D11.selectedIndex;


   
   //T-business fix ip
   document.ZDAT2.T21.value = '<%ejGet(T_BusinessFesUserName)%>';
   document.ZDAT2.P21.value = top.frames[0].cfgprovpwd[2];
   document.ZDAT2.P22.value = top.frames[0].cfgprovpwd[2];
   
   document.ZDAT2.T23.value = '<%ejGet(T_BusinessFesMtu)%>';
   onDemand[2] = '<%ejGet(T_BusinessFesOnDemand)%>';
   isIdleTimeOut[2] = '<%ejGet(T_BusFesIsIdleTimeOut)%>';
   
   if (isIdleTimeOut[2] == '1' )
      document.ZDAT2.R21[1].checked = true;
   else
      document.ZDAT2.R21[0].checked = true;
   
      if (onDemand[2] == '1') {
       document.ZDAT2.C21.checked = true;
   }else{
       document.ZDAT2.C21.checked = false;
   }
   document.ZDAT2.D21.selectedIndex = '<%ejGet(T_BusinessFesTimeOut)%>';
   top.frames[0].tbusfes_name = document.ZDAT2.T21.value;
   top.frames[0].tbusfes_password = document.ZDAT2.P21.value;
   top.frames[0].tbusfes_mtu = document.ZDAT2.T23.value;
   top.frames[0].tbusfes_persist = isIdleTimeOut[2];
   top.frames[0].tbusfes_ondemand = onDemand[2];
   top.frames[0].tbusfes_timeout = document.ZDAT2.D21.selectedIndex;

   //another provider. 
   var dslIpAddr = '<%ejGet(pppFixIpAddress)%>';
   var dnsPrimaryAddr = '<%ejGet(dnsPrimary)%>';
   var dnsSecondaryAddr = '<%ejGet(dnsSecondary)%>';

    
   document.ZDAT3.T31.value = '<%ejGet(pppUserName)%>';
   document.ZDAT3.P31.value = top.frames[0].cfgprovpwd[3];
   document.ZDAT3.P32.value = top.frames[0].cfgprovpwd[3];
   
   document.ZDAT3.T32.value = '<%ejGet(pppServerName)%>';

   if(dslIpAddr == '')
		dslIpAddr = '0.0.0.0';

   var ipSplited = dslIpAddr.split('.');
   document.ZDAT3.T33_1.value = ipSplited[0];
   document.ZDAT3.T33_2.value = ipSplited[1];
   document.ZDAT3.T33_3.value = ipSplited[2];
   document.ZDAT3.T33_4.value = ipSplited[3];

   var addrPart = dnsPrimaryAddr.split('.');
   document.ZDAT3.T34_1.value = addrPart[0];
   document.ZDAT3.T34_2.value = addrPart[1];
   document.ZDAT3.T34_3.value = addrPart[2];
   document.ZDAT3.T34_4.value = addrPart[3];

   var addrPartSecond = dnsSecondaryAddr.split('.');
   document.ZDAT3.T35_1.value = addrPartSecond[0];
   document.ZDAT3.T35_2.value = addrPartSecond[1];
   document.ZDAT3.T35_3.value = addrPartSecond[2];
   document.ZDAT3.T35_4.value = addrPartSecond[3];

   document.ZDAT3.T36.value = '<%ejGet(ethMtu)%>';

   onDemand[3] = '<%ejGet(enblOnDemand)%>';
   isIdleTimeOut[3] = '<%ejGet(pppIsIdleTimeOut)%>';
   if (isIdleTimeOut[3] == '1' )
      document.ZDAT3.R31[1].checked = true;
   else
      document.ZDAT3.R31[0].checked = true;
   if (onDemand[3] == '1') {
      document.ZDAT3.C31.checked = true;
   }else{
      document.ZDAT3.C31.checked = false;
   }
   document.ZDAT3.D31.selectedIndex = '<%ejGet(pppTimeOut)%>';
   top.frames[0].another_name = document.ZDAT3.T31.value;
   top.frames[0].another_password = document.ZDAT3.P31.value;
   top.frames[0].another_servicename = document.ZDAT3.T32.value;
   top.frames[0].another_fixip = dslIpAddr;
   top.frames[0].another_dns = dnsPrimaryAddr;
   top.frames[0].another_dns2 = dnsSecondaryAddr;
   top.frames[0].another_mtu = document.ZDAT3.T36.value;
   top.frames[0].another_persist = isIdleTimeOut[3];
   top.frames[0].another_ondemand = onDemand[3];
   top.frames[0].another_timeout = document.ZDAT3.D31.selectedIndex;

   stAkt();

   stDis();

   alert_wan_test_status();
}


function IsPppoeChanged(){
	
	var persist;
	var ondemand;
	var tmpIP;
	var checkIP;
	var seindex = document.F1.D1.selectedIndex;

	if (top.frames[0].ActiveServiceID == 0) {
		return true;
	}
	if((seindex==0) && (document.ZDAT.P1.value==''))
	    return false;
        if((seindex==1) && (document.ZDAT1.T11.value==''))
	    return false;
	if((seindex==2) && (document.ZDAT2.T21.value==''))
	    return false;
	if((seindex==3) && (document.ZDAT3.T31.value==''))
	    return false;

	if(top.frames[0].ActiveServiceID == 1){
		if(top.frames[0].tonline_password1 == '')
		    return false;
		if(document.F1.D1.selectedIndex != 0)
			return true;
		else{
			if(top.frames[0].tonline_password1 != document.ZDAT.P1.value)
				return true;
			if(top.frames[0].tonline_number != document.ZDAT.T1.value)
				return true;
			if(top.frames[0].tonline_suffix != document.ZDAT.T2.value)
				return true;
			if(top.frames[0].tonline_password != document.ZDAT.P2.value)
				return true;
			if(top.frames[0].tonline_mtu != document.ZDAT.T3.value)
				return true;
			persist = (document.ZDAT.R1[1].checked == true)?1:0;
			if(top.frames[0].tonline_persist != persist)
				return true;
			ondemand = (document.ZDAT00.C1.checked == true)?1:0;
			if(top.frames[0].tonline_ondemand != ondemand)
				return true;
			if(top.frames[0].tonline_timeout != document.ZDAT00.D1.selectedIndex)
				return true;
		}
	}
	if(top.frames[0].ActiveServiceID == 2){
		if(top.frames[0].tbusdyn_name == '')
		    return false;
		if(document.F1.D1.selectedIndex != 1)
			return true;
		else{
			if(top.frames[0].tbusdyn_name != document.ZDAT1.T11.value)
				return true;
			if(top.frames[0].tbusdyn_password != document.ZDAT1.P11.value)
				return true;
			if(top.frames[0].tbusdyn_mtu != document.ZDAT1.T12.value)
				return true;
			persist = (document.ZDAT1.R11[1].checked == true)?1:0;
			if(top.frames[0].tbusdyn_persist != persist)
				return true;
			ondemand = (document.ZDAT1.C11.checked == true)?1:0;
			if(top.frames[0].tbusdyn_ondemand != ondemand)
				return true;
			if(top.frames[0].tbusdyn_timeout != document.ZDAT1.D11.selectedIndex)
				return true;
		}
	}
	if(top.frames[0].ActiveServiceID == 3){
		if(top.frames[0].tbusfes_name == '')
		    return false;
		if(document.F1.D1.selectedIndex != 2)
			return true;
		else{
			if(top.frames[0].tbusfes_name != document.ZDAT2.T21.value)
				return true;
			if(top.frames[0].tbusfes_password != document.ZDAT2.P21.value)
				return true;
			if(top.frames[0].tbusfes_mtu != document.ZDAT2.T23.value)
				return true;
			persist = (document.ZDAT2.R21[1].checked == true)?1:0;
			if(top.frames[0].tbusfes_persist != persist)
				return true;
			ondemand = (document.ZDAT2.C21.checked == true)?1:0;
			if(top.frames[0].tbusfes_ondemand != ondemand)
				return true;
			if(top.frames[0].tbusfes_timeout != document.ZDAT2.D21.selectedIndex)
				return true;
		}

	}
	if(top.frames[0].ActiveServiceID == 4){
		if(top.frames[0].another_name == '')
		    return false;
		if(document.F1.D1.selectedIndex != 3)
			return true;
		else{
			if(top.frames[0].another_name != document.ZDAT3.T31.value)
				return true;
			if(top.frames[0].another_password != document.ZDAT3.P31.value)
				return true;
			if(top.frames[0].another_servicename != document.ZDAT3.T32.value)
				return true;
			tmpIP = document.ZDAT3.T33_1.value + '.' + document.ZDAT3.T33_2.value + '.' + document.ZDAT3.T33_3.value + '.' + document.ZDAT3.T33_4.value;
			checkIP = top.frames[0].another_fixip;
			if((checkIP == '') || (checkIP == '&nbsp') || (checkIP == '0.0.0.0')){
				if((tmpIP != '0.0.0.0') && (document.ZDAT3.T33_1.value != ''))
					return true;
			}
			else{
				if(tmpIP != checkIP)
					return true;
			}
			
			tmpIP = document.ZDAT3.T34_1.value + '.' + document.ZDAT3.T34_2.value + '.' + document.ZDAT3.T34_3.value + '.' + document.ZDAT3.T34_4.value;
			checkIP = top.frames[0].another_dns;
			if((checkIP == '') || (checkIP == '&nbsp') || (checkIP == '0.0.0.0')){
				if((tmpIP != '0.0.0.0') && (document.ZDAT3.T34_1.value != ''))
					return true;
			}
			else{
				if(tmpIP != checkIP)
					return true;
			}
				
			tmpIP = document.ZDAT3.T35_1.value + '.' + document.ZDAT3.T35_2.value + '.' + document.ZDAT3.T35_3.value + '.' + document.ZDAT3.T35_4.value;
			checkIP = top.frames[0].another_dns2;
			if((checkIP == '') || (checkIP == '&nbsp') || (checkIP == '0.0.0.0')){
				if((tmpIP != '0.0.0.0') && (document.ZDAT3.T35_1.value != ''))
					return true;
			}
			else{
				if(tmpIP != checkIP)
					return true;
			}
			if(top.frames[0].another_mtu != document.ZDAT3.T36.value)
				return true;
			persist = (document.ZDAT3.R31[1].checked == true)?1:0;
			if(top.frames[0].another_persist != persist)
				return true;
			ondemand = (document.ZDAT3.C31.checked == true)?1:0;
			if(top.frames[0].another_ondemand != ondemand)
				return true;
			if(top.frames[0].another_timeout != document.ZDAT3.D31.selectedIndex)
				return true;
		}
	}
	
	return false;
}

function stAkt(){
  document.getElementById("c_um0").style.visibility='hidden';
  document.getElementById("c_um00").style.visibility='hidden';
  document.getElementById("c_um01").style.visibility='hidden';
  document.getElementById("c_um1").style.visibility='hidden';
  document.getElementById("c_um2").style.visibility='hidden';
  document.getElementById("c_um3").style.visibility='hidden';
  document.getElementById("c_um" + document.F1.D1.selectedIndex).style.visibility='visible';

  switch(document.F1.D1.selectedIndex){
    case 0: 
      document.ZDAT.P1.focus();
      stAkt2();
      break;
    case 1: document.ZDAT1.T11.focus(); break;
    case 2: document.ZDAT2.T21.focus(); break;
    case 3: document.ZDAT3.T31.focus(); break;
  }

  stDis();
}

function stAkt2(){
  document.getElementById("c_um00").style.visibility='hidden';
  document.getElementById("c_um01").style.visibility='hidden';
  if (document.ZDAT.R1[0].checked == true){
    document.getElementById("c_um00").style.visibility='visible';
  }
  else{
    document.getElementById("c_um01").style.visibility='visible';
  }

}

function i_prfElem(nr){ 
  switch(document.F1.D1.selectedIndex){
    case 0:
      break;
    case 1:
      if(document.ZDAT1.R11[0].checked == false){
        i_showElem(19);
      }
      else{
        i_showElem(nr);
      }
      break;
    case 2:
      if(document.ZDAT2.R21[0].checked == false){
        i_showElem(19);
      }
      else{
        i_showElem(nr);
      }
      break;
    case 3:
      if(document.ZDAT3.R31[0].checked == false){
        i_showElem(19);
      }
      else{
        i_showElem(nr);
      }
      break;
  }
}

function stDis(){ 
  var state = false;
	var col = "#FFFFFF";
	
  switch(document.F1.D1.selectedIndex){
	case 0:
      stAkt2();
		break;
	case 1:
  	  if (document.ZDAT1.R11[0].checked == false){
    		state = true;
    		col = "#D4D0C8";
  	  }
		document.ZDAT1.C11.disabled = state;
		document.ZDAT1.D11.style.backgroundColor = col;
		document.ZDAT1.D11.disabled = state;
		break;
	case 2:
  	  if (document.ZDAT2.R21[0].checked == false){
    		state = true;
    		col = "#D4D0C8";
  	  }
		document.ZDAT2.C21.disabled = state;
		document.ZDAT2.D21.style.backgroundColor = col;
		document.ZDAT2.D21.disabled = state;
		break;
	case 3:
  	  if (document.ZDAT3.R31[0].checked == false){
    		state = true;
    		col = "#D4D0C8";
  	  }
		document.ZDAT3.C31.disabled = state;
		document.ZDAT3.D31.style.backgroundColor = col;
		document.ZDAT3.D31.disabled = state;
		break;
	}
}

function txtHinw(feldname){
  return "Im Feld '" + feldname + "' wurde eine falsche Eingabe bzw. keine Eingabe vorgenommen. " + 
         "\nDie korrekte Eingabe ist notwendig, um eine Internetverbindung aufbauen zu können.";
}

function txtProv(){
  return "Achtung, bitte beachten Sie, dass keine Verbindung ins Internet möglich ist,"+
         "\nwenn Sie diesen Provider auswählen, jedoch keine Internet Zugangsdaten eingeben."+
         "\n"+
         "\nKlicken Sie auf 'Abbrechen', um Ihre Internet Zugangsdaten jetzt einzugeben"+
         "\noder klicken Sie auf 'OK', um ohne Eingabe der Zugangsdaten fortzufahren.";
}

function txtDel(){
  return "Achtung, bitte beachten Sie, dass keine Verbindung ins Internet möglich ist,"+
         "\nwenn Sie den Provider löschen. Wählen Sie daher nach dem Löschen einen" +
         "\nanderen Provider aus, indem Sie nochmals in 'Internetzugang' einsteigen."+
         "\n"+
         "\nKlicken Sie auf 'Abbrechen', um den Provider beizubehalten"+
         "\noder klicken Sie auf 'OK', um den Provider zu löschen.";
}

function stDel(){
	
  var loc;
    
  var provok=confirm(txtDel());
  if(provok==true){
  switch(document.F1.D1.selectedIndex){
    case 0:
      document.ZDAT.P1.value = "";
      document.ZDAT.T1.value = "";
      document.ZDAT.T2.value = "";
      document.ZDAT.P2.value = "";
      document.ZDAT.T3.value = "1492";
      document.ZDAT00.C1.checked = true;
      document.ZDAT00.D1.selectedIndex = 0;
      loc = 'hcti_deletewan.cgi?DelWhichWan=1';
      break;
  
    case 1:
      document.ZDAT1.T11.value = "";
      document.ZDAT1.P11.value = "";
      document.ZDAT1.T12.value = "1492";
      document.ZDAT1.C11.checked = true;
      document.ZDAT1.D11.selectedIndex = 0;
      loc = 'hcti_deletewan.cgi?DelWhichWan=2';
      break;
  
    case 2:
      document.ZDAT2.T21.value = "";
      document.ZDAT2.P21.value = "";
      document.ZDAT2.T23.value = "1456";
      document.ZDAT2.C21.checked = true;
      document.ZDAT2.D21.selectedIndex = 0;
      loc = 'hcti_deletewan.cgi?DelWhichWan=3';
      break;
  
    case 3:
      document.ZDAT3.T31.value = "";
      document.ZDAT3.P31.value = "";
      document.ZDAT3.T32.value = "";
      document.ZDAT3.T36.value = "1454";
      document.ZDAT3.C31.checked = true;
      document.ZDAT3.D31.selectedIndex = 0;
      loc = 'hcti_deletewan.cgi?DelWhichWan=4';
      break;
  }
  document.getElementById("c_verzoeg1").style.visibility='visible';
  document.getElementById("c_verzoeg2").style.visibility='visible';
  document.getElementById("t_but1").style.visibility='hidden';
  document.getElementById("t_but2").style.visibility='hidden';
  document.getElementById("t_but3").style.visibility='hidden';
  document.getElementById("t_but4").style.visibility='hidden';

  //Select-Options müssen extra ausgeblendet werden!
  document.F1.D1.style.visibility='hidden';
  document.ZDAT00.D1.style.visibility='hidden';
  document.ZDAT1.D11.style.visibility='hidden';
  document.ZDAT2.D21.style.visibility='hidden';
  document.ZDAT3.D31.style.visibility='hidden';

  var code = 'location.assign("' + loc + '")';
  eval(code);
  }
}

function stOk(){
  var provok;
  switch(document.F1.D1.selectedIndex){
  case 0:
    if (document.ZDAT.P1.value!="" || document.ZDAT.T1.value!="" || document.ZDAT.T2.value!="" || document.ZDAT.P2.value!="" || document.ZDAT.P3.value!=""){
    if (document.ZDAT.P1.value=="" || isInvalidNumber(document.ZDAT.P1.value)){
      alert(txtHinw("Anschlusskennung"));
      document.ZDAT.P1.value = "";
      document.ZDAT.P1.focus();
      return false;
    }
    if (document.ZDAT.T1.value=="" || isInvalidNumber(document.ZDAT.T1.value)){
      alert(txtHinw("T-Online-Nummer"));
      document.ZDAT.T1.value = "";
      document.ZDAT.T1.focus();
      return false;
    }
    if (document.ZDAT.T2.value=="" || isInvalidNumber(document.ZDAT.T2.value)){
      alert(txtHinw("Mitbenutznummer"));
      document.ZDAT.T2.value = "";
      document.ZDAT.T2.focus();
      return false;
    }
    if (document.ZDAT.P2.value=="" || document.ZDAT.P3.value=="" || isInvalidString(document.ZDAT.P2.value) || document.ZDAT.P2.value!=document.ZDAT.P3.value){
      alert(txtHinw("Persönliches Kennwort' oder 'Kennwort Wiederholung"));
      document.ZDAT.P2.value = "";
      document.ZDAT.P3.value = "";
      document.ZDAT.P2.focus();
      return false;
    }
    if(document.ZDAT.T3.value<1200 || document.ZDAT.T3.value > 1492){
	alert("Der eingegebene Wert ist unzulÿssig.\nEs muss eine Zahl zwischen 1200 und 1492");
	document.ZDAT.T3.value = "";
	document.ZDAT.T3.focus();
	return false;
    }


    if (document.ZDAT.T3.value=="" || isNaN(document.ZDAT.T3.value)){
      alert(txtHinw("MTU (Standardwert 1492)"));
      document.ZDAT.T3.value = "";
      document.ZDAT.T3.focus();
      return false;
    }
  }
  else{
    provok=confirm(txtProv());
    if(provok==true){
      document.ZDAT.T3.value = "1492";
      document.ZDAT00.C1.checked = true;
      document.ZDAT00.D1.selectedIndex = 0;
    }
    else return false;
  }
  break;
  
  case 1:
    if (document.ZDAT1.T11.value!="" || document.ZDAT1.P11.value!="" || document.ZDAT1.P12.value!=""){
    if (document.ZDAT1.T11.value==""){
      alert(txtHinw("Benutzername"));
      document.ZDAT1.T11.focus();
      return false;
    }
    if (document.ZDAT1.P11.value=="" || document.ZDAT1.P12.value=="" || isInvalidString(document.ZDAT1.P11.value) || document.ZDAT1.P11.value!=document.ZDAT1.P12.value){
      alert(txtHinw("Kennwort' oder 'Kennwort Wiederholung"));
      document.ZDAT1.P11.value = "";
      document.ZDAT1.P12.value = "";
      document.ZDAT1.P11.focus();
      return false;
    }
    if(document.ZDAT1.T12.value<1200 || document.ZDAT1.T12.value > 1492){
	alert("Der eingegebene Wert ist unzulÿssig.\nEs muss eine Zahl zwischen 1200 und 1492");
	document.ZDAT1.T12.value = "";
	document.ZDAT1.T12.focus();
	return false;
    }


    if (document.ZDAT1.T12.value=="" || isNaN(document.ZDAT1.T12.value)){
      alert(txtHinw("MTU (Standardwert 1492)"));
      document.ZDAT1.T12.value = "";
      document.ZDAT1.T12.focus();
      return false;
    }
  }
  else{
    provok=confirm(txtProv());
    if(provok==true){
      document.ZDAT1.T12.value = "1492";
      document.ZDAT1.C11.checked = true;
      document.ZDAT1.D11.selectedIndex = 0;
    }
    else return false;
  }
  break;
  
  case 2:
  if (document.ZDAT2.T21.value!="" || document.ZDAT2.P21.value!="" || document.ZDAT2.P22.value!=""){
  if (document.ZDAT2.T21.value==""){
      alert(txtHinw("Benutzername"));
      document.ZDAT2.T21.focus();
      exit;
    }
    if (document.ZDAT2.P21.value=="" || document.ZDAT2.P22.value=="" || isInvalidString(document.ZDAT2.P21.value) || document.ZDAT2.P21.value!=document.ZDAT2.P22.value){
      alert(txtHinw("Kennwort' oder 'Kennwort Wiederholung"));
      document.ZDAT2.P21.value = "";
      document.ZDAT2.P22.value = "";
      document.ZDAT2.P21.focus();
      return false;
    }
    if(document.ZDAT2.T23.value<1200 || document.ZDAT2.T23.value > 1492){
	alert("Der eingegebene Wert ist unzulÿssig.\nEs muss eine Zahl zwischen 1200 und 1492");
	document.ZDAT2.T23.value = "";
	document.ZDAT2.T23.focus();
	return false;
    }
    if (document.ZDAT2.T23.value=="" || isNaN(document.ZDAT2.T23.value)){
      alert(txtHinw("MTU (Standardwert 1456)"));
      document.ZDAT2.T23.value = "";
      document.ZDAT2.T23.focus();
      return false;
    }
  }else{
    provok=confirm(txtProv());
    if(provok==true){
      document.ZDAT2.T23.value = "1456";
      document.ZDAT2.C21.checked = true;
      document.ZDAT2.D21.selectedIndex = 0;
    }
    else return;
  }

  break;
  
  case 3:
    if (document.ZDAT3.T31.value!="" || document.ZDAT3.P31.value!="" || document.ZDAT3.P32.value!="" || document.ZDAT3.T32.value!=""){
            if (document.ZDAT3.T31.value=="" || isInvalidString(document.ZDAT3.T31.value)){
              alert(txtHinw("Benutzername"));
              document.ZDAT3.T31.focus();
              return false;
            }
	    if (document.ZDAT3.P31.value=="" || document.ZDAT3.P32.value=="" || isInvalidString(document.ZDAT3.P31.value) || document.ZDAT3.P31.value!=document.ZDAT3.P32.value){
              alert(txtHinw("Kennwort' oder 'Kennwort Wiederholung"));
              document.ZDAT3.P31.value = "";
              document.ZDAT3.P32.value = "";
              document.ZDAT3.P31.focus();
              return false;
	    }
	    if (document.ZDAT3.T32.value!="" && isInvalidString(document.ZDAT3.T32.value)){
              alert(txtHinw("Name des Dienstes"));
	      document.ZDAT3.T32.focus();
              return false;
            }

            if (isNIP(document.ZDAT3.T33_1,document.ZDAT3.T33_2,document.ZDAT3.T33_3,document.ZDAT3.T33_4) > 0){
              alert("Bitte tragen Sie eine korrekte IP-Adresse als feste IP-Adresse ein");
              return false;
            }
            if (isNIP(document.ZDAT3.T34_1,document.ZDAT3.T34_2,document.ZDAT3.T34_3,document.ZDAT3.T34_4) > 0){
              alert("Bitte tragen Sie eine korrekte IP-Adresse für den primären DNS-Server ein");
              return false;
            }
            if (isNIP(document.ZDAT3.T35_1,document.ZDAT3.T35_2,document.ZDAT3.T35_3,document.ZDAT3.T35_4) > 0){
              alert("Bitte tragen Sie eine korrekte IP-Adresse für den sekundären DNS-Server ein");
              return false;
            }
	    if(document.ZDAT3.T36.value<1200 || document.ZDAT3.T36.value > 1492){
		alert("Der eingegebene Wert ist unzulÿssig.\nEs muss eine Zahl zwischen 1200 und 1492");
		document.ZDAT3.T36.value = "";
		document.ZDAT3.T36.focus();
		return false;
	    }
	    if (document.ZDAT3.T36.value=="" || isNaN(document.ZDAT3.T36.value)){
              alert(txtHinw("MTU (Standardwert 1454)"));
              document.ZDAT3.T36.value = "";
              document.ZDAT3.T36.focus();
              return false;
            }
          }
          else{
            provok=confirm(txtProv());
            if(provok==true){
              document.ZDAT3.T36.value = "1454";
              document.ZDAT3.C31.checked = true;
              document.ZDAT3.D31.selectedIndex = 0;
            }
	    else 
	    	return false;
          }
          break;
  }

  stSave();
}

var loc;
function stApply(){
    var code = 'location.assign("' + loc + '")';
    eval(code);
 }

 
function stSave(){	
  switch(document.F1.D1.selectedIndex){
    //T-online 
    case 0:
    if(IsPppoeChanged() == true ) {
//	if(confirm(rebootconfirm) == true )
	    loc = 'hcti_netzwerk_wan_tonline_reboot.cgi?';
//	else
//	    return false;
    }
    else
    loc = 'hcti_netzwerk_wan_tonline_waiting.cgi?';
      loc += 'serviceId=1';      
      loc += '&provtxt1=' + encodeUrl(document.ZDAT.P1.value);
      loc += '&provtxt2=' + encodeUrl(document.ZDAT.T1.value);
      loc += '&provtxt3=' + encodeUrl(document.ZDAT.T2.value);
      loc += '&provpwd=' + encodeUrl(document.ZDAT.P2.value);
      loc += '&T_onLineEthMtu='+encodeUrl(document.ZDAT.T3.value);
      loc += ('&T_onLineIsIdleTimeOut=' + ((document.ZDAT.R1[1].checked == true)?"1":"0"));
      if(document.ZDAT.R1[0].checked==true){
           loc += ('&T_onLineOnDemand=' + ((document.ZDAT00.C1.checked == true)?"1":"0"));
	   loc += '&T_onLinePppIdleTimeOut=' + document.ZDAT00.D1.selectedIndex;
      }
      loc += '&redial_time_tonline=' + document.ZDAT01.D2.selectedIndex;
      break;
    //T-business dynamic ip
    case 1:
    if(IsPppoeChanged() == true ){
//	if(confirm(rebootconfirm) == true )
	    loc = 'hcti_netzwerk_wan_tbusinessdyn_reboot.cgi?';
//	else
//	    return false;
    }
      else
      	loc = 'hcti_netzwerk_wan_tbusinessdyn_waiting.cgi?';
      loc += 'serviceId=2';      
      loc += '&provtxt1=' + encodeUrl(document.ZDAT1.T11.value);
      loc += '&provpwd=' + encodeUrl(document.ZDAT1.P11.value);
      loc += '&T_BusinessDynMtu='+encodeUrl(document.ZDAT1.T12.value);
      loc += ('&T_BusinessDynIsIdleTimeOut=' + ((document.ZDAT1.R11[1].checked == true)?"1":"0"));
      if(document.ZDAT1.R11[0].checked==true){
          loc += ('&T_BusinessDynOnDemand=' + ((document.ZDAT1.C11.checked == true)?"1":"0"));
	  loc += '&T_BusinessDynTimeOut=' + document.ZDAT1.D11.selectedIndex;
      }
      break;
    //T-business fix ip
    case 2:
    if(IsPppoeChanged() == true ){
	//if(confirm(rebootconfirm) == true )
	    loc = 'hcti_netzwerk_wan_tbusinessfes_reboot.cgi?';
	//else
	//    return false;
    }
    else
	loc = 'hcti_netzwerk_wan_tbusinessfes_waiting.cgi?';
      loc += 'serviceId=3';
      loc += '&provtxt1=' + encodeUrl(document.ZDAT2.T21.value);
      loc += '&provpwd=' + encodeUrl(document.ZDAT2.P21.value);
      loc += '&T_BusinessFesMtu='+encodeUrl(document.ZDAT2.T23.value);
      loc += ('&T_BusFesIsIdleTimeOut=' + ((document.ZDAT2.R21[1].checked == true)?"1":"0"));
      if(document.ZDAT2.R21[0].checked==true){
          loc += ('&T_BusinessFesOnDemand=' + ((document.ZDAT2.C21.checked == true)?"1":"0"));
	  loc += '&T_BusinessFesTimeOut=' + document.ZDAT2.D21.selectedIndex;
      }
      break;
    //another provider
    case 3:
    if(IsPppoeChanged() == true ){
	//if(confirm(rebootconfirm) == true )
	    loc = 'hcti_netzwerk_wan_providerx_reboot.cgi?';
	//else
	//    return false;
    }
	else
      		loc = 'hcti_netzwerk_wan_providerx_waiting.cgi?';
      loc += 'serviceId=4';
      loc += '&provtxt1=' + encodeUrl(document.ZDAT3.T31.value);      
      loc += '&provpwd=' + encodeUrl(document.ZDAT3.P31.value);
      loc += '&provtxt2=' + encodeUrl(document.ZDAT3.T32.value);
      var pppFixVal;
      var dnsVal1;
      var dnsVal2;
      //if no access data, ip and dns should all be null
      if(document.ZDAT3.T31.value==''){
	loc += '&pppFixIpAddress=' + "0.0.0.0";
	loc += '&dnsPrimary=' + "0.0.0.0";
	loc += '&dnsSecondary=' + "0.0.0.0";
      }
      else
      {
	  //fix ip
	  if (document.ZDAT3.T33_1.value == "" &&
		  document.ZDAT3.T33_2.value == "" &&
		  document.ZDAT3.T33_3.value == "" &&
		  document.ZDAT3.T33_4.value == "" ) {
		  loc += '&pppFixIpAddress=' + "0.0.0.0";
	  } else {
		  pppFixVal = document.ZDAT3.T33_1.value + '.'+ 
					  document.ZDAT3.T33_2.value + '.'+ 
					  document.ZDAT3.T33_3.value + '.'+ 
					  document.ZDAT3.T33_4.value;
		  loc += '&pppFixIpAddress=' + pppFixVal;

	  }
	  //dns primary
	  if (document.ZDAT3.T34_1.value == "" &&
		  document.ZDAT3.T34_2.value == "" &&
		  document.ZDAT3.T34_3.value == "" &&
		  document.ZDAT3.T34_4.value == "" ) {
		  loc += '&dnsPrimary=' + "0.0.0.0";
	  } else {
		  dnsVal1 = document.ZDAT3.T34_1.value + '.' + 
					document.ZDAT3.T34_2.value + '.' + 
					document.ZDAT3.T34_3.value + '.' + 
					document.ZDAT3.T34_4.value;
		  loc += '&dnsPrimary=' + dnsVal1;	 
	  }
	  //dns secondary
	  if (document.ZDAT3.T35_1.value == "" &&
		  document.ZDAT3.T35_2.value == "" &&
		  document.ZDAT3.T35_3.value == "" &&
		  document.ZDAT3.T35_4.value == "") {
		  loc += '&dnsSecondary=' + "0.0.0.0";
	  } else {
		  dnsVal2 = document.ZDAT3.T35_1.value + '.'+ 
					document.ZDAT3.T35_2.value + '.'+ 
					document.ZDAT3.T35_3.value + '.'+ 
					document.ZDAT3.T35_4.value;
		  loc += '&dnsSecondary=' + dnsVal2;
	  
	  }
      }
      loc += '&ethMtu=' + encodeUrl(document.ZDAT3.T36.value);
      loc += ('&pppIsIdleTimeOut=' + ((document.ZDAT3.R31[1].checked == true)?"1":"0"));
      if(document.ZDAT3.R31[0].checked==true){
          loc += ('&enblOnDemand=' + ((document.ZDAT3.C31.checked == true)?"1":"0"));
	  loc += '&pppTimeOut='+ document.ZDAT3.D31.selectedIndex;
      }
     break;
  }
  document.getElementById("c_verzoeg1").style.visibility='visible';
  document.getElementById("c_verzoeg2").style.visibility='visible';
  document.getElementById("t_but1").style.visibility='hidden';
  document.getElementById("t_but2").style.visibility='hidden';
  document.getElementById("t_but3").style.visibility='hidden';
  document.getElementById("t_but4").style.visibility='hidden';

  //Select-Options müssen extra ausgeblendet werden!
  document.F1.D1.style.visibility='hidden';
  document.ZDAT00.D1.style.visibility='hidden';
  document.ZDAT01.D2.style.visibility='hidden';
  document.ZDAT1.D11.style.visibility='hidden';
  document.ZDAT2.D21.style.visibility='hidden';
  document.ZDAT3.D31.style.visibility='hidden';
  setTimeout("stApply()", 1000);
}
  
function stZrk(){
  top.frames[2].location.href = "hcti_netzwerk.htm";
}

